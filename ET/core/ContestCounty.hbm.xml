<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="edu.uwec.cs.cs355.group4.et.core.ContestCounty, ElectionTracker" table="contestcounty">
		<id name="ID" column="ContestCountyID" type="Int64">
			<generator class="identity"/>
		</id>
    <bag lazy="true" inverse="true" name="ResponseValues" cascade="all-delete-orphan" >
      <key column="ContestCountyID"/>
      <one-to-many class="edu.uwec.cs.cs355.group4.et.core.ResponseValue, ElectionTracker"/>
    </bag>
		<property name="WardsReporting" column="WardsReporting" type="Int32" length="100" not-null="true"/>
		<property name="WardCount" column="WardCount" type="Int32" length="100" not-null="true"/>
		<many-to-one name="County" class="edu.uwec.cs.cs355.group4.et.core.County, ElectionTracker" column="CountyID" not-null="true" />
		<many-to-one name="ElectionContest" class="edu.uwec.cs.cs355.group4.et.core.ElectionContest, ElectionTracker" column="ElectionContestID" not-null="true"/>
	</class>
</hibernate-mapping>
