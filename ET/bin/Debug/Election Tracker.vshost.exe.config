<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <sectionGroup name="applicationSettings" type="System.Configuration.ApplicationSettingsGroup, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" >
      <section name="edu.uwec.cs.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
    </sectionGroup>
    <sectionGroup name="spring">
      <section name="context" type="Spring.Context.Support.ContextHandler, Spring.Core"/>
      <section name="objects" type="Spring.Context.Support.DefaultSectionHandler, Spring.Core" />
    </sectionGroup>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler,log4net" />
  </configSections>
  <spring>
    <context>
      <resource uri="config://spring/objects"/>
    </context>
    <objects>
      <description>An  example that demonstrates simple IoC features.</description>
      <object name="Election"
        type="edu.uwec.cs.et.core.Election, edu.uwec.cs.et.core">
      </object>
    </objects>
  </spring>
  <log4net>
    <appender name="ConsoleAppender" type="log4net.Appender.ConsoleAppender">
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level %logger - %message%newline" />
      </layout>
    </appender>

    <!-- Set default logging level to DEBUG -->
    <root>
      <level value="DEBUG" />
      <appender-ref ref="ConsoleAppender" />
    </root>

    <!-- Set logging for Spring to INFO.  Logger names in Spring correspond to the namespace -->
    <logger name="Spring">
      <level value="INFO" />
    </logger>
  </log4net>
  
  <connectionStrings>
    <add name="edu.uwec.cs.Properties.Settings.DB_1_ConnectionString"
        connectionString="Data Source =&quot;.\MyDatabase#1.sdf&quot;"
        providerName="Microsoft.SqlServerCe.Client" />
  </connectionStrings>
  <applicationSettings>
    <edu.uwec.cs.Properties.Settings>
    </edu.uwec.cs.Properties.Settings>
  </applicationSettings>
</configuration>